<h1>Review: <%= @review.junior_consultant %> - <%= @review.review_type %></h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Project</th>
      <th>Date Submitted</th>
      <th colspan="3">Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @review.feedbacks.each do |feedback| %>
      <tr>
        <td><%= feedback.user %></td>
        <td><%= feedback.project_worked_on %></td>
        <td><%= feedback.created_at.to_s %></td>
        <td><%= link_to 'Show', [@review, feedback] %></td>
        <td><%= link_to 'Edit', edit_review_feedback_path(@review, feedback) %></td>
        <td><%= link_to 'Destroy', [@review, feedback], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New Feedback', new_review_feedback_path(@review) %><br /><br />

<%= link_to 'Edit', edit_review_path(@review) %> |
<%= link_to 'Back', reviews_path %>
