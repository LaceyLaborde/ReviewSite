<% if @invitations_received.count > 0 %>
  <h2>Provide Feedback For</h2>
  <%= render :partial => 'invitations/invitations_received' %>
<% end %>

<% if @reviews.count > 0 %>
  <% @reviews.each do |review| %>
    <%= render :partial => 'reviews/review', :locals => { review: review } %>
  <% end %>
<% end %>